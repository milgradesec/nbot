{
  "Statement": [
    {
      "Action": ["s3:GetBucketLocation"],
      "Effect": "Allow",
      "Principal": {
        "AWS": ["*"]
      },
      "Resource": ["arn:aws:s3:::nbot"]
    },
    {
      "Action": ["s3:ListBucket"],
      "Condition": {
        "StringEquals": {
          "s3:prefix": ["clips", "img", "minitas"]
        }
      },
      "Effect": "Allow",
      "Principal": {
        "AWS": ["*"]
      },
      "Resource": ["arn:aws:s3:::nbot"]
    },
    {
      "Action": ["s3:GetObject"],
      "Effect": "Allow",
      "Principal": {
        "AWS": ["*"]
      },
      "Resource": [
        "arn:aws:s3:::nbot/minitas*",
        "arn:aws:s3:::nbot/clips*",
        "arn:aws:s3:::nbot/img*"
      ]
    }
  ],
  "Version": "2012-10-17"
}
